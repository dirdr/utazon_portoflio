{
  "name": "utazon_portfolio",
  "private": true,
  "version": "0.8.5",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run update-meta && tsc && vite build",
    "build:staging": "VITE_SITE_URL=https://staging.utazon.fr npm run build",
    "build:production": "VITE_SITE_URL=https://utazon.fr npm run build",
    "update-meta": "tsx scripts/generate-html-meta.ts",
    "generate:sitemap": "tsx scripts/generate-sitemap.ts",
    "generate:robots": "tsx scripts/generate-robots.ts",
    "sitemap:docker": "npm run generate:sitemap",
    "robots:docker": "npm run generate:robots",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.9",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "lenis": "^1.3.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hover-video-player": "^10.0.2",
    "react-i18next": "^15.6.0",
    "react-intersection-observer": "^9.16.0",
    "react-player": "^3.3.1",
    "sitemap": "^8.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "wouter": "^3.1.2",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "tailwindcss-debug-screens": "^3.0.1",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
